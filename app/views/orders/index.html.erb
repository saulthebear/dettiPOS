<h1>Orders</h1>

<table class="table">
  <thead>
    <tr>
      <th scope="col">Order #</th>
      <th scope="col">Price</th>
      <th scope="col">Date / Time</th>
      <th scope="col"></th>
    </tr>
  </thead>

  <tbody>
    <% @orders.each do |order| %>
      <%= render 'order', order: order %>
    <% end %>
  </tbody>
</table>
